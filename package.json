  "license": "MIT",
  "author": "xud6",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "https://miguelcobain.github.io/ember-paper",
    "before": "ember-cli-sri",
    "versionCompatibility": {
      "ember": ">=3.20.0"
